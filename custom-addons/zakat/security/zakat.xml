<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data noupdate="0">

    <record id="group_registration_user" model="res.groups">
        <field name="name">Registration Group</field>
        <field name="category_id" ref="base.module_category_accounting_and_finance"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_departmental_user" model="res.groups">
        <field name="name">Departmental Group</field>
        <field name="category_id" ref="base.module_category_accounting_and_finance"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_social_survey" model="res.groups">
        <field name="name">Social Survey</field>
        <field name="category_id" ref="base.module_category_accounting_and_finance"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_central_team" model="res.groups">
        <field name="name">Central Team</field>
        <field name="category_id" ref="base.module_category_accounting_and_finance"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="case_study_registration_user" model="ir.rule">
        <field name="name">Registration User</field>
        <field name="model_id" ref="model_case_study_request"/>
        <field name="global" eval="True"/>
        <field name="groups" eval="[(4, ref('group_registration_user'))]"/>
        <field name="domain_force">[('state','=','new')]</field>
    </record>

   
</data>

</openerp>