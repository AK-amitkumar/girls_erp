        <record id="seq_medical_patient" model="ir.sequence">
            <field name="name">Patient</field>
            <field name="code">medical.patient</field>
            <field name="prefix">PAC</field>
            <field name="padding">3</field>
        </record>
        
        <record id="seq_medical_lab_appointment" model="ir.sequence">
            <field name="name">Lab</field>
            <field name="code">medical.lab.appointment</field>
            <field name="prefix">APT</field>
            <field name="padding">3</field>
        </record>        
        
        <record id="seq_medical_lab_test" model="ir.sequence">
            <field name="name">Lab Test</field>
            <field name="code">medical.lab</field>
            <field name="prefix">TEST</field>
            <field name="padding">3</field>
        </record> 
        
        <record id="seq_medical_patient_lab_test" model="ir.sequence">
            <field name="name">Lab Patient Test</field>
            <field name="code">medical.patient.lab.test</field>
            <field name="prefix">LABT</field>
            <field name="padding">3</field>
        </record>        
        
    </data>
</openerp>
